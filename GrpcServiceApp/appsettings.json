{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "AzureAd": {
    "Authority": "https://login.microsoftonline.com/{Your Azure AD Tenant ID}/",
    "Audience": "Your server side Application ID URI(api://appid)"
  }
}
